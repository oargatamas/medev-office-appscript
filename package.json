{
  "name": "medev-office-appscript-webclient",
  "version": "1.0.0",
  "description": "Medev Project effort sheet utility application based on React and deployed on Google App Script. ",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "glogin": "clasp login",
    "glogout": "clasp logout",
    "gcreate": "clasp create --title medev-office-client --rootDir ./app-script",
    "gclone": "clasp clone 1CylsEPXJB1EYlzGYPVJsrTpXTvXhfgfVMnr1SvjdBCgRiVuFJslNQu3Y --rootDir ./app-script",
    "gpush": "clasp push",
    "gpull": "clasp pull",
    "gstart": "clasp push --watch",
    "start": "parcel src/index.html --dist-dir app-script",
    "build": "parcel build src/index.html --dist-dir app-script"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oargatamas/medev-office-appscript.git"
  },
  "author": "Tamás Dániel Oarga",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oargatamas/medev-office-appscript/issues"
  },
  "homepage": "https://github.com/oargatamas/medev-office-appscript#readme",
  "staticFiles": [
    {
      "staticPath": "src/api",
      "staticOutPath": "api"
    },
    {
      "staticPath": "src/appsscript.json"
    },
    {
      "staticPath": "src/main.js"
    }
  ],
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@types/google-apps-script": "^1.0.57",
    "parcel": "^2.8.3",
    "parcel-reporter-static-files-copy": "^1.5.0",
    "process": "^0.11.10"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.6",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "sort-by": "^1.2.0"
  }
}
